{
  "name": "@ipare/core",
  "version": "1.7.2",
  "description": "面向云的现代渐进式轻量 Node.js 框架",
  "engines": {
    "node": ">=12.16.0"
  },
  "homepage": "https://github.com/ipare/ipare",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipare/ipare"
  },
  "keywords": [
    "ipare",
    "serverless"
  ],
  "bugs": {
    "url": "https://github.com/ipare/ipare/issues",
    "email": "support@hal.wang"
  },
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "rimraf ./dist/* && tsc",
    "prepublish": "npm run build",
    "publish:beta": "npm run build && npm publish --tag=beta --access=public",
    "lint": "eslint src test --ext .ts",
    "pretest": "npm run build",
    "test": "jest"
  },
  "author": "hal-wang",
  "license": "MIT",
  "dependencies": {
    "http-status-codes": "^2.2.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7"
  },
  "files": [
    "/.github/*",
    "/coverage/*",
    "/dist/*",
    "/src/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
    ".gitignore",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
