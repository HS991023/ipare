{
  "name": "@ipare/router",
  "version": "2.1.9",
  "engines": {
    "node": ">=12.16.0"
  },
  "description": "Ipare 的路由插件",
  "homepage": "https://github.com/ipare/router",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipare/router"
  },
  "keywords": [
    "ipare",
    "restful",
    "router"
  ],
  "bugs": {
    "url": "https://github.com/ipare/router/issues",
    "email": "support@hal.wang"
  },
  "main": "dist/index.js",
  "scripts": {
