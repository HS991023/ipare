    "lint": "eslint src test --ext .ts"
  },
  "author": "hal-wang",
  "license": "MIT",
  "dependencies": {
    "@ipare/core": "^1.6.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@ipare/core": "~1"
  },
  "peerDependenciesMeta": {
    "@ipare/core": {
      "optional": true
    }
  },
  "files": [
    "/.github/*",
    "/dist/*",
    "/scripts/*",
    "/src/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
