{
  "compilerOptions": {
    "lib": [
      "esnext"
    ],
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true
  },
  "include": [
    "src/**/*.ts"
  ]
}