{
  "name": "@ipare/swagger",
  "version": "1.1.0",
  "description": "使用 swagger 自动生成你的 Ipare 文档",
  "engines": {
    "node": ">=12.16.0"
  },
  "homepage": "https://github.com/ipare/swagger",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipare/swagger"
  },
  "keywords": [
    "ipare",
    "swagger"
  ],
  "bugs": {
    "url": "https://github.com/ipare/swagger/issues",
    "email": "support@hal.wang"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "sh scripts/build.sh",
    "publish:beta": "npm run build && npm publish --tag=beta --access=public",
    "test": "npm run build && jest",
    "tests": "sh scripts/tests.sh",
    "lint": "eslint src test --ext .ts"
  },
  "author": "hal-wang",
  "license": "MIT",
  "dependencies": {
    "@ipare/core": "^1.6.3",
    "@ipare/inject": "^1.2.3",
    "@ipare/pipe": "^1.3.5",
    "@ipare/router": "^2.1.7",
    "openapi3-ts": "^2.0.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@ipare/http": "^1.2.4",
    "@types/jest": "^28.1.5",
    "@types/node": "^18.0.4",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "codecov": "^3.8.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.6",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@ipare/core": "~1",
    "@ipare/inject": "~1",
    "@ipare/pipe": "~1",
    "@ipare/router": "~2"
  },
  "peerDependenciesMeta": {
    "@ipare/core": {
      "optional": true
    },
    "@ipare/inject": {
      "optional": true
    },
    "@ipare/router": {
      "optional": true
    },
    "@ipare/pipe": {
      "optional": true
    }
  },
  "files": [
    "/.github/*",
    "/dist/*",
    "/scripts/*",
    "/src/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
    ".gitignore",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md",
    "jest.config.js",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
