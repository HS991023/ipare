{
  "name": "@ipare/testing",
  "version": "0.0.6",
  "description": "Ipare 的测试工具",
  "engines": {
    "node": ">=12.16.0"
  },
  "homepage": "https://github.com/ipare/testing",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipare/testing"
  },
  "keywords": [
    "ipare",
    "test"
  ],
  "bugs": {
    "url": "https://github.com/ipare/testing/issues",
    "email": "support@hal.wang"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "sh scripts/build.sh",
    "prepublish": "npm run build",
    "publish:beta": "sh scripts/build.sh && npm publish --tag=beta --access=public",
    "tests": "sh scripts/tests.sh",
    "lint": "eslint src test --ext .ts"
  },
  "author": "hal-wang",
  "license": "MIT",
  "dependencies": {
    "@ipare/core": "^1.6.3",
    "@ipare/http": "^1.2.4",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.12",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.5",
    "supertest": "^6.2.4"
  },
  "devDependencies": {
    "@types/node": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@ipare/core": "~1",
    "@ipare/http": "~1"
  },
  "peerDependenciesMeta": {
    "@ipare/core": {
      "optional": true
    },
    "@ipare/http": {
      "optional": true
    }
  },
  "files": [
    "/.github/*",
    "/dist/*",
    "/scripts/*",
    "/src/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
    ".gitignore",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
